// Create a jenkins pipeline job with multiple stages with different slave on each stage-First 5 stages should run on NODE1 and next 5 stages should run on NODE2

pipeline {

    agent none 

    stages {

        stage('Node 1 Work') {

            agent { label 'node1' }

            steps {

                script {

                    for (int i = 1; i <= 5; i++) {

                        echo "Stage ${i} is running on Node 1"

                    }

                }

            }

        }

        stage('Node 2 Work') {

            agent { label 'node2' }

            steps {

                script {

                    for (int i = 6; i <= 10; i++) {

                        echo "Stage ${i} is running on Node 2"

                    }

                }

            }

        }

    }

}
 
